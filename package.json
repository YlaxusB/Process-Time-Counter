{
  "name": "processtimecounter",
  "productName": "Process Time Counter",
  "version": "1.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:5000",
  "homepage": "./",
  "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
  "devDependencies": {
    "types": "electron",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^18.14.6",
    "appdata-path": "^1.0.0",
    "axios": "^1.1.3",
    "chart.js": "^4.2.1",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "fs": "^0.0.1-security",
    "http-proxy-middleware": "^2.0.6",
    "promisify": "^0.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "0.8.5",
    "save-file": "^2.3.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4",
    "write-json-file": "^5.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start && set NODE_ENV=dev && node app.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "processTimeCounter.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "extends": null,
    "files": [
      "./build/**/*",
      "./public/electron.js"
    ],
    "extraFiles": [
      {
        "from": "assets",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "@electron/remote": "^2.0.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "app-data-folder": "^1.0.0",
    "appdata-path": "^1.0.0",
    "axios": "^1.1.3",
    "chart.js": "^4.0.0",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "faker": "^6.6.6",
    "http-proxy-middleware": "^2.0.6",
    "menu": "^0.2.5",
    "promisify": "^0.0.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-native-fs": "^2.0.1-rc.2",
    "react-scripts": "0.8.5",
    "recharts": "^2.4.3",
    "save-file": "^2.3.1",
    "uglify-js": "^3.17.4",
    "username": "^6.0.0",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4",
    "write-json-file": "^5.0.0",
    "yarn": "^1.22.19"
  }
}